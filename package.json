{
  "name": "kikstart-ui-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "build": "ng build --prod",
    "postbuild": "cp -v dist/kikstart-ui-starter/index.html dist/kikstart-ui-starter/404.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@kikstart/ui": "1.7.0",
    "@ngx-formly/bootstrap": "5.5.6",
    "@ngx-formly/core": "5.5.6",
    "bootstrap": "4.4.1",
    "bootswatch": "4.4.1",
    "font-awesome": "4.7.0",
    "ngx-bootstrap": "5.2.0",
    "ngx-timeago": "1.0.2",
    "ngx-toastr": "11.2.1",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.20",
    "@angular/cli": "8.3.20",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.16",
    "codelyzer": "5.2.0",
    "husky": "3.1.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "protractor": "5.4.2",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beeman/kikstart-ui-starter.git"
  }
}
